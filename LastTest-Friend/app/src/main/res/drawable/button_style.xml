<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <stroke
                android:color="#ff9bd69b"
                android:width="1dp"
                />
            <solid
                android:color="#ff9bd59a"
                />

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:color="#ff606367"
                android:width="1dp"
                />
            <solid
                android:color="#ff606367"
                />
        </shape>
    </item>
</selector>